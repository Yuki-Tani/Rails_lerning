$("#micropost-<%= @micropost.id %> .favorite").html("<%= escape_javascript(render 'microposts/favor_form', micropost: @micropost) %>");
$("#favorites-<%= current_user.id %>").html("<%= current_user.fav_posts.count %>");
$("#stars-<%= @micropost.user_id %>").html("<%= User.find_by(id:  @micropost.user_id).microposts.joins(:favorites).count %>");
